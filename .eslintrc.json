{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"airbnb-base"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"warn",
			"tab"
		],
		"quotes": [
			"warn",
			"single"
		],
		"no-unused-vars": "warn",
		"no-console": "off",
		"func-names": "off"
	}
}